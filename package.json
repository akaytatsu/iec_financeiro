    "date-fns": "^2.29.1",
    "element-plus": "^2.2.9",
    "vue": "^3.2.37",
    "vue-axios": "^3.4.1",
    "vue-material-design-icons": "^5.1.2",
    "vue-number-format": "^1.3.1",
    "vue-router": "^4.1.3",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
